# Файловое хранилище
# Возможные значения: 'local' | 's3'
FILE_STORAGE_TYPE=local

# URL фронтенда для генерации ссылок приглашений
FRONTEND_URL=http://localhost:3000

# Настройки для S3 (используются когда FILE_STORAGE_TYPE=s3)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_S3_BUCKET_NAME=your-bucket-name
AWS_S3_BASE_URL=https://your-bucket-name.s3.amazonaws.com

# MongoDB (если используется)
MONGODB_URI=mongodb://localhost:27017/smm

# JWT секрет
JWT_SECRET=your-jwt-secret-here

# Другие настройки приложения
NODE_ENV=development
PORT=3000
